<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/main_navigation"
    app:startDestination="@id/main_fragment">
    <fragment
        android:id="@+id/main_fragment"
        android:name="com.noboauto.demoapp.ui.home.MainFragment"
        android:label="MainFragment" >
        <action
            android:id="@+id/action_main_to_photo"
            app:destination="@id/photo_fragment" />
        <action
            android:id="@+id/action_main_to_article"
            app:destination="@id/article_fragment" />
    </fragment>
    <fragment
        android:id="@+id/photo_fragment"
        android:name="com.noboauto.demoapp.ui.photo.PhotoListFragment"
        android:label="PhotoFragment" />
    <fragment
        android:id="@+id/article_fragment"
        android:name="com.noboauto.demoapp.ui.article.ArticleListFragment"
        android:label="ArticleFragment" />
</navigation>